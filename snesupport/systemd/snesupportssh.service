[Unit]
Description=Reverse proxy port 22
After=syslog.target network.target multi-user.target 
# nodm.service
# Requires=nodm.service

[Service]
Type=simple
Environment="SSH_REMOTE=1919"
ExecStart=/usr/bin/ssh turn@cloud.sneconsulting.co.uk -R ${SSH_REMOTE}:localhost:22 -N
Restart=always
RestartSec=3


[Install]
WantedBy=multi-user.target

